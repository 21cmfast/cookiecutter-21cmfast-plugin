    "codacy_projectid" : "[Get ID from https://app.codacy.com/gh/{{ cookiecutter.repo_username }}/{{ cookiecutter.repo_name }}/settings]",
    "codeclimate" : ["no", "yes"],
    "sphinx_docs": ["yes", "no"],
